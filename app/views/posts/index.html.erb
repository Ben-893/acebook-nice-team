<% @posts.each do |post| %>

  <div>
    <p id="messageBox"><%= post.message %></p>
  </div>

  <p>Created at <%= post.created_at.strftime("%e-%m-%y %H:%M") %> by <%= post.poster_name %><br><br>

  <% if post.like_id(post.id) %>
    <%= button_to "delete like", {:controller => :likes,
                                  :action => 'destroy', :id => post.like_id(post.id) },
                                  :method => :delete %>
  <% else %>
    # Yvonne's front end here
  <% end %>

  <%= link_to new_post_path do %>
      New post
  <% end %>
<% end %>